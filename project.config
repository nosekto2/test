[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/nosekto2
[access "refs/for/refs/*"]
	read = group user/nosekto2
	push = group user/nosekto2
	label-Code-Review = -2..+2 group user/nosekto2
	label-Verified = -1..+1 group user/nosekto2
	rebase = group user/nosekto2
	removeReviewer = group user/nosekto2
	submit = group user/nosekto2
[access "refs/heads/*"]
	read = group user/nosekto2
	create = group user/nosekto2
	push = +force group user/nosekto2
	pushMerge = group user/nosekto2
[access "refs/tags/*"]
	push = +force group user/nosekto2
	pushTag = group user/nosekto2
	pushSignedTag = group user/nosekto2
